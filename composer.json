{
  "name": "mario-naether/chromiuman",
  "description": "Codeception Extension for automatically starting and stoping Chromedriver when running tests ",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Mario NÃ¤ther",
      "email": "mario-naether@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "codeception/codeception": "^2.3||^3.0||^4.0",
    "lbaey/chromedriver": "^1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "extra": {
    "lbaey/chromedriver": {
      "bypass-select" : true,
      "chromedriver-version": "74.0.3729.6"
    }
  },
  "autoload": {
    "classmap": [
      "src/Codeception",
      "src/Chromiuman"
    ]
  },
  "scripts": {
    "check": "phpcs",
    "fix": "phpcbf"
  }
}
